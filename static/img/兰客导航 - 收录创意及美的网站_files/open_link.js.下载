function search(sender) {

	var keyword = document.getElementById("search").value

	if (keyword == "") {
		window.open(searchURL[sender.id].home)
	} else {
		var url = searchURL[sender.id].search
		var url = url.replace("%keyword%",keyword)
		window.open(url)
	}	
}
